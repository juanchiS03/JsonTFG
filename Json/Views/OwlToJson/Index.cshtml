@{
    ViewData["Title"] = "Convert OWL to JSON";
}

<h2>@ViewData["Title"]</h2>

<form method="post" enctype="multipart/form-data" action="/OwlToJson/ConvertOwlToJson">
    <div class="form-group">
        <label for="owlFile">Select OWL File</label>
        <input type="file" name="owlFile" id="owlFile" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Convert to JSON</button>
</form>
